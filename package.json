{
  "name": "redux-typescript-webpack",
  "version": "0.0.1",
  "description": "redux-typescript-webpack",
  "main": "dist/main.js",
  "repository": {
    "type": "git",
    "url": "git@gitlab.ucloudadmin.com:buhi/redux-typescript-webpack.git"
  },
  "scripts": {
    "build": "npm run build:prod && npm run build:dev",
    "build:prod": "webpack --bail --progress --profile",
    "build:dev": "webpack --bail --progress --profile",
    "dev": "webpack-dev-server --devtool=source-map --host=0.0.0.0 --history-api-fallback --inline --progress --port 9001",
    "server": "NODE_ENV=prod node server.js",
    "server:dev": "NODE_ENV=dev node server.js",
    "test": "karma start",
    "test:watch": "karma start --auto-watch --no-single-run",
    "deploy": "npm run build && cd dist && rm last -rf && mv current last && mv `date +'%Y-%m-%d'` current",
    "deploy:dev": "npm run build:dev && cd dist && rm last -rf && mv current last && mv `date +'%Y-%m-%d'` current"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "font-awesome": "^4.7.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0",
    "redux-form": "^6.1.1",
    "whatwg-fetch": "^1.0.0"
  },
  "keywords": [
    "redux-typescript-webpack"
  ],
  "author": "YS",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.5.0",
    "awesome-typescript-loader": "^2.2.4",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.25.0",
    "es6-promise": "^4.0.3",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "null-loader": "^0.1.1",
    "postcss-loader": "^0.13.0",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.13.1",
    "typescript": "^2.0.3",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  }
}
